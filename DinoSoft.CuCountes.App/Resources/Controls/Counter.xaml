<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DinoSoft.CuCountes.App.Resources.Controls.Counter"
             xmlns:viewmodels="clr-namespace:DinoSoft.CuCountes.App.ViewModels"
             xmlns:controls="clr-namespace:DinoSoft.CuCountes.App.Resources.Controls"
             x:DataType="viewmodels:CounterVM">
    <ContentView.Content>
        <StackLayout>
            <Label Text="{Binding Model.Name}" />
            <Label Text="{Binding Model.Value}" />
            <Label Text="13" />
        </StackLayout>

 
    </ContentView.Content>
</ContentView>